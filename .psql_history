CREATE VIEW habitacion_vista(id_habitacion, nombre_habitacion, precio_habitacion, id_hotel) ASSELECT id_habitacion, nombre_habitacion, precio_habitacion, id_hotelFROM hoteles_habitaciones;
CREATE TABLE HabitacionesSELECT h1.id_habitacion, h1.nombre_habitacion, h1.precio_habitacion, h1.id_hotelFROM habitacion_vista AS h1, habitacion_vista AS h2WHERE h1.id_habitacion = h2.id_habitacion;
CREATE TABLE Habitaciones ASSELECT h1.id_habitacion, h1.nombre_habitacion, h1.precio_habitacion, h1.id_hotelFROM habitacion_vista AS h1, habitacion_vista AS h2WHERE h1.id_habitacion = h2.id_habitacion;
